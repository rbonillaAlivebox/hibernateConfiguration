package com.alivebox.model;

import javax.persistence.Entity;
import javax.persistence.SequenceGenerator;
import javax.persistence.Table;

/**
 * Address Model Class to model the ADDRESS table
 * @author Alivebox
 * @since 11-03-2014
 */
@Entity
@Table(name="ADDRESS")
@SequenceGenerator(name="SEQ_ADDRESS", sequenceName="SEQ_ADDRESS", initialValue=1)
public class Address extends BaseEntity{
	//*******************
	// CONSTANTS - START
	//*******************
	
	private static final long serialVersionUID = 201403110924L;
	
	//*****************
	// CONSTANTS - END
	//*****************
	
	
	//*******************
	// VARIABLES - START
	//*******************
	
	private Integer id;
	private String city;
	private String country;
	private String address1;
	private String address2;
	
	//*****************
	// VARIABLES - END
	//*****************
	
	
	//*********************
	// CONSTRUCTOR - START
	//*********************
	
	public Address(){
		super();
	}
	
	//*******************
	// CONSTRUCTOR - END
	//*******************
	
	
	//*****************************
	// GETTERS AND SETTERS - START
	//*****************************
	
	public Integer getId() {
		return id;
	}
	public void setId(Integer id) {
		this.id = id;
	}
	
	public String getCity() {
		return city;
	}
	public void setCity(String city) {
		this.city = city;
	}
	
	public String getCountry() {
		return country;
	}
	public void setCountry(String country) {
		this.country = country;
	}
	
	public String getAddress1() {
		return address1;
	}
	public void setAddress1(String address1) {
		this.address1 = address1;
	}
	
	public String getAddress2() {
		return address2;
	}
	public void setAddress2(String address2) {
		this.address2 = address2;
	}
	
	//***************************
	// GETTERS AND SETTERS - END
	//***************************
}
